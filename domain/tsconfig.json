{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "../dist/domain",
     "types": ["node"],
    "declaration": true,
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "composite": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
